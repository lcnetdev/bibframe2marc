    <x:expect label="...$b generated from EDTF or dateTime dates" test="//marc:datafield[@tag='045']/marc:subfield[@code='b'][1] = 'd19270401' and //marc:datafield[@tag='045']/marc:subfield[@code='b'][2] = 'd20190413'"/>
